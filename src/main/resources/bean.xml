<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="Person" class="com.helloworld.helloworld.Person" init-method="init" destroy-method="destroy">
    <property name="gender" value="Male/Female" />
    </bean>

    <bean id="address" class="com.helloworld.helloworld.Address">
    <property name="city" value="Mountain View"/>
    <property name="state" value="California"/>
    
    </bean>

</beans>

